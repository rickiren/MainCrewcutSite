{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron .",
    "dev:full": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "api:market": "ts-node scripts/fetchMarketData.ts",
    "api:websocket": "ts-node scripts/streamLivePrices.ts",
    "api:metadata": "ts-node scripts/fetchTickerMetadata.ts",
    "api:news": "ts-node scripts/fetchNews.ts",
    "api:ipos": "ts-node scripts/fetchIpos.ts",
    "api:low-float": "ts-node scripts/filterLowFloatTickers.ts",
    "api:hod-scanner": "ts-node scripts/highOfDayScanner.ts",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete all",
    "build": "vite build",
    "build:electron": "npm run build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.5",
    "dotenv": "^17.2.0",
    "lucide-react": "^0.344.0",
    "moment-timezone": "^0.6.0",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@solana/wallet-standard-features": "^1.3.0",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "openai": "^5.8.3",
    "pm2": "^5.3.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "wait-on": "^8.0.3"
  }
}
